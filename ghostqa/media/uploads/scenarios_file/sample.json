      "TestSuite1": {
        "name": "Test Suite 1 - Login Tests",
        "testCases": [
          {
            "TestCase1": {
              "name": "Test Case 1 - Login with Standard User",
              "beforeEach": [
                {
                  "type": "visit",
                  "selector": "https://www.saucedemo.com/v1/"
                }
              ],
              "actions": [
                {
                  "type": "type",
                  "selector": "input#user-name",
                  "text": "standard_user"
                },
                {
                  "type": "type",
                  "selector": "input#password",
                  "text": "secret_sauce"
                },
                {
                  "type": "click",
                  "selector": "input#login-button"
                }
              ]
            }
          },
